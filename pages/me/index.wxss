/**index.wxss**/
.container {
  
} 

.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #aaa;
}

.me-tabbar {
  display: flex;
  margin: 30rpx;
  padding: 20rpx;
  border-radius: 10rpx;
  overflow: hidden;
  background: rgba(192, 187, 187, 0.185);
}

.me-tabbar-item {
  flex: 1;
  text-align: center;
  padding: 30rpx 0;
  border-right: 1px solid rgba(192, 187, 187, 1);
}

.me-tabbar-item:last-child {
  border-right: 0;
}

.me-tabbar-icon{
  width:66rpx;
  height: 66rpx;
  margin-bottom: 16rpx;
}
